{
  "name": "nuxt-typescript-starter",
  "version": "1.0.4",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "nuxt": "^1.0.0-alpha.5",
    "nuxt-class-component": "^1.0.1",
    "vue-class-component": "^5.0.2",
    "vue-property-decorator": "^5.1.1"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "test": "jest"
  },
  "devDependencies": {
    "@types/node": "^8.0.14",
    "babel-jest": "21.2.0",
    "babel-preset-es2015": "6.24.1",
    "jest": "21.2.1",
    "jest-vue-preprocessor": "1.2.0",
    "ts-loader": "^2.3.0",
    "typescript": "^2.4.1",
    "vue-test-utils": "1.0.0-beta.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "~/(components|plugins|services)/([^\\.]*)$": "<rootDir>/$1/$2"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  }
}
